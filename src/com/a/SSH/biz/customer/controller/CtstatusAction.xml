<struts>
    <package name="ctstatus" namespace="/ctstatus" extends="default">
        <action name="toMain" class="ctstatusAction" method="toMain">
            <result name="customerAdmin" >
                /WEB-INF/view/customer/ctstatus.jsp
            </result>
        </action>
        <action name="queryCtstatus" class="ctstatusAction" method="queryCtstatus">
        </action>
        <action name="saveOrUpdate" class="ctstatusAction" method="saveOrUpdate">
        </action>
        <action name="deleteAuth" class="ctstatusAction" method="deleteAuth">
        </action>
    </package>
</struts>
